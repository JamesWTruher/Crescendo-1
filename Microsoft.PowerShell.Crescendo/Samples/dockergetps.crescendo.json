{
   "$schema": "https://raw.githubusercontent.com/PowerShell/Crescendo/master/Microsoft.PowerShell.Crescendo/src/Microsoft.PowerShell.Crescendo.Schema.json",
   "Commands": [
        {
            "Verb": "Get",
            "Noun": "DockerPs",
            "OriginalName": "docker",
            "OriginalCommandElements": [
                "ps",
                "--format",
                "{{json .}}"
            ],
            "OutputHandlers": [
                {
                "ParameterSetName": "Default",
                "Handler": "$args[0] | convertfrom-json | Add-Member -TypeName DockerProcess -PassThru"
                }
            ]
        
        }
    ]
}
